{
  "Kafka": {
    "Brokers": "localhost:9092"
  },
  "Azure": {
    "EventHub": "Endpoint=sb://slimmessagebus2.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey={{azure_eventhub_key}}",
    "Storage": "DefaultEndpointsProtocol=https;AccountName=slimmessagebus2;AccountKey={{azure_eventhub_storage_key}};EndpointSuffix=core.windows.net",
    "ContainerName": "leases"
  },
  "LogConfiguration": {
    "factoryAdapter": {
      "type": "Common.Logging.Simple.DebugLoggerFactoryAdapter, Common.Logging",
      "arguments": {
        "level": "All",
        "showDateTime": true,
        "showLogName": true,
        "showLevel": true,
        "dateTimeFormat": "yyyy/MM/dd HH:mm:ss.fff"
      }
    }
  }
}

